{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "singularName": "student",
    "pluralName": "students",
    "displayName": "Student"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "student_id": {
      "type": "biginteger",
      "required": true,
      "unique": true
    },
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "last_name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "year": {
      "type": "integer",
      "required": true
    },
    "student_image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "mapping": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::mapping.mapping",
      "inversedBy": "students"
    }
  }
}
